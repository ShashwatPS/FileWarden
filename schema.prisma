datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

model Post {
  id          String   @default(cuid()) @id
  createdAt   DateTime @db.Date
  publishTime DateTime @db.Time()
  filePath    String
  fileName    String
  type        String
}